{
    "private": true,
    "scripts": {
        "start": "dotnet fable watch ./src -o ./public --run parcel serve public/index.html",
        "build": "dotnet fable ./src -o ./public --run parcel build public/index.html --public-url .",
        "clean": "rm -rf dist public/*.js public/.fable .parcel-cache dist"
    },
    "devDependencies": {
        "@babel/core": "^7.10.5",
        "@babel/plugin-transform-modules-commonjs": "^7.10.4",
        "@babel/preset-react": "^7.10.4",
        "parcel": "^2.0.0-beta.2"
    },
    "dependencies": {
        "@emotion/css": "11.1.3",
        "react": "^16.13.1",
        "react-dom": "^16.13.1"
    }
}
